	using System;
					
	public class Program
	{
		public static void Main()
		{
						
			int length = 30; // pwedeng baguhin yung number
			int width = 2;
			int area = length * width;
			Console.WriteLine("Given the following rectangle measurements: ");
			Console.WriteLine("Length = "+ length) ; Console.WriteLine("Width = "+ width);
			Console.WriteLine("The area of rectangle is: "+ area);
		}
	}
